    "repair": "./bin/wee-repair",
    "install-deps": "./scripts/install-dependencies.sh",
    "startup": "./bin/wee-startup",
    "wee-start": "./bin/wee-startup --silent",
    "test:unit": "jest tests/unit --detectOpenHandles",
    "test:integration": "jest tests/integration --detectOpenHandles",
